{
    "name": "vxm/laravel-view-localization",
    "description": "Support dynamic render view by user locale for your Laravel application.",
    "keywords": [
        "vxm",
        "laravel-package",
        "laravel-view-localization",
        "l10n",
        "localization"
    ],
    "homepage": "https://github.com/vuongxuongminh/laravel-view-localization",
    "license": "MIT",
    "authors": [
        {
            "name": "Vuong Xuong Minh",
            "email": "vuongxuongminh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.7"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "~7.5",
        "scrutinizer/ocular": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "VXM\\ViewLocalization\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VXM\\ViewLocalization\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "VXM\\ViewLocalization\\ViewLocalizationServiceProvider"
            ]
        }
	}
}
